{
  "name": "ci-pilot",
  "version": "1.8.2",
  "description": "Automate your CI pipeline with ease - you'll find binaries and functions that help you simplify the process",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "git@github.com:ultm8soulja/ci-pilot.git",
  "author": "Colin Agbabiaka <coliny2k@gmail.com>",
  "engines": {
    "node": ">=10.13.0"
  },
  "license": "MIT",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "git",
    "npm",
    "gitflow",
    "githubflow",
    "publish",
    "release",
    "prerelease"
  ],
  "bin": {
    "ci-pilot": "./dist/cli/index.js"
  },
  "scripts": {
    "clear": "rimraf dist node_modules yarn-error.log",
    "reset": "yarn clear && yarn",
    "clean": "rimraf dist",
    "compile": "tsc",
    "compile:watch": "yarn compile -- --watch",
    "compile:dry": "yarn compile --noEmit",
    "compile:dry:watch": "yarn compile:dry -- --watch",
    "build": "yarn clean && yarn compile && yarn permissions",
    "commit:prep": "yarn clean && yarn build && yarn lint && yarn format && yarn test",
    "format": "prettier --write \"src/**/*.*\" *.config.js",
    "lint": "eslint . --ext .js,.ts,.json",
    "lint:fix": "yarn lint --fix",
    "test": "jest --passWithNoTests",
    "permissions": "shx chmod +x dist/cli/index.js",
    "dev": "ts-node src/cli/index.ts",
    "start": "node ./dist/cli/index.js",
    "release": "semantic-release",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate"
  },
  "devDependencies": {
    "@commitlint/cli": ">=7.6.0",
    "@commitlint/config-conventional": "^8.3.4",
    "@schemastore/package": "^0.0.5",
    "@semantic-release/changelog": "^4.0.0",
    "@semantic-release/git": "^8.0.0",
    "@types/chalk": "^2.2.0",
    "@types/chance": "^1.0.10",
    "@types/clear": "^0.1.0",
    "@types/conventional-commits-detector": "^1.0.0",
    "@types/conventional-recommended-bump": "^6.0.0",
    "@types/figlet": "^1.2.0",
    "@types/find-root": "^1.1.1",
    "@types/inquirer": "^6.5.0",
    "@types/jest": "^25.1.4",
    "@types/lodash": "^4.14.150",
    "@types/minimist": "^1.2.0",
    "@types/semver": "^7.1.0",
    "@types/shelljs": "^0.8.7",
    "@types/standard-version": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "all-contributors-cli": "^6.16.0",
    "chance": "^1.1.4",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-json": "^2.1.1",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "jest": "^25.2.3",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "semantic-release": "^16.0.4",
    "shx": "^0.3.2",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.10.1",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "clear": "^0.1.0",
    "conventional-commits-detector": "^1.0.2",
    "conventional-recommended-bump": "^6.0.9",
    "dependency-graph": "^0.9.0",
    "dotenv": "^8.2.0",
    "env-var": "^6.1.1",
    "figlet": "^1.2.4",
    "find-root": "^1.1.0",
    "gh-got": "^9.0.0",
    "inquirer": "^7.1.0",
    "lodash": "^4.17.15",
    "minimist": "^1.2.5",
    "semver": "^7.3.2",
    "shelljs": "^0.8.4",
    "simple-git": "^2.3.0",
    "standard-version": "^8.0.0"
  }
}
